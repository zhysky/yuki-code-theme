	"semanticHighlighting": true,
	"semanticTokenColors": {
		"newOperator": "#0000ff",
		"stringLiteral": "#a31515",
		"customLiteral": "#000000",
		"numberLiteral": "#098658",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				// "fontStyle": "italic",
				"foreground": "#5e93ac"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.other", //其它变量，如 $some_variables。
				"variable.other.readwrite", //let、var声明的变量的颜色
				"punctuation.definition.variable", //the $ in PHP and Shell.
				"variable.other.constant", //const声明的不可变变量的颜色
				"variable.language", //语言保留变量，如 this、super、self 等。this、super等
				"variable.parameter", //函数参数
				"variable.other.member", //类或其他数据结构的字段，属性，成员
				"variable.function", //函数和方法名称的作用域，但仅限于调用时	
				"variable.annotation", //作为数值一部分的标识符中的最终标签
				"punctuation.definition.annotation",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#00c5df"
			}	
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#9dc3ee"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#f026c4bd"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#895eff"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#b388ff"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#61AFEF"
			}
		},
		{
			"name": "Other Variable, String Link",
